<launch>
  <!-- Launch an example that sends commands via C++ API. -->

  <arg name="config" default="$(find moveit_servoing)/config/panda_simulated_config.yaml"/>

  <node name="servo_server" pkg="moveit_servo" type="servo_server" output="screen" >
    <rosparam command="load" file="$(arg config)"/>
  </node>

  <node name="moveit_servoing" pkg="moveit_servoing" type="moveit_servoing" output="screen">
    <rosparam command="load" file="$(arg config)" />
    <rosparam command="load" file="$(find moveit_servoing)/config/pose_tracking_settings.yaml" />
  </node>

</launch>
